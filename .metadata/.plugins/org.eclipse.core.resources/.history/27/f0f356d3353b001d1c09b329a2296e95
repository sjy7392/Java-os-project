
public class Hw3_13 {

     public static void main(String[] args) {
         int x , y;
         for(int i = 1; i < 100; i++) {
             int c = 0; x = i % 10; y = i / 10;
             if ((x == 3)||(x == 6)||(x == 9)) c++;
             if ((y == 3)||(y == 6)||(y == 9)) c++;
             if (c == 2) System.out.println(i+"박수 두번");
             else if( c == 1) System.out.println(i+"박수 한번");
             else System.out.println(i); // 숫자 입력을 하기 싫을 때 주석 처리
         }

     }

 }